{
  "name": "ejfox",
  "version": "1.0.0",
  "description": "EJ Fox.com",
  "author": "EJ Fox <ejfox@ejfox.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "serve": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "books:scrape-highlights": "scrape-highlights https://www.goodreads.com/notes/9273959-ej-fox > static/data/book_highlights.json",
    "blog:to-md": "node_modules/processmd/cli.js content/blog/posts/*.json --outputDir content/blog/md/ --convertMode source",
    "blog:md-to-json": "node_modules/processmd/cli.js content/blog/md/* --outputDir content/blog/posts/",
    "dropbox-import": "cp ~/Dropbox/Writing/PUBLISHED/*.md content/blog/md/;npm run blog:md-to-json",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "keybase": "nuxt generate;yes | cp -rf dist/* /keybase/public/ejfox/;echo 'Copied to Keybase public folder'",
    "publish": "npx csvtojson static/data/projects.csv > static/data/projects.json;npm run dropbox-import;npm run keybase;git stage -A;git commit -m 'build: publish from npm run publish';git push;echo 'Published on Keybase and GitHub (via Netlify)'",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/feed": "^1.1.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "chance": "^1.1.7",
    "cheerio": "^1.0.0-rc.5",
    "cloudinary": "^1.23.0",
    "d3": "^5.16.0",
    "dotenv": "^8.2.0",
    "exif-date": "^2.1.0",
    "exifreader": "^2.13.1",
    "fs": "0.0.1-security",
    "howler": "^2.2.1",
    "insane": "^2.6.2",
    "libxmljs": "^0.19.5",
    "lodash": "^4.17.20",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-github-headings": "^1.1.2",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "markdown-it-toc-and-anchor": "^4.2.0",
    "marked": "^0.7.0",
    "nuxt": "^1.4.5",
    "nuxt-purgecss": "^0.2.1",
    "slug": "^1.1.0",
    "slugify": "^1.4.6",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "truncate": "^2.0.1",
    "uri-js": "^4.4.1",
    "url-parse": ">=1.5.0",
    "uslug": "^1.0.4",
    "vue-howler": "^0.7.0",
    "vue-moment": "^4.1.0",
    "vue-style-loader": "^4.1.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "glob": "^7.1.6",
    "highlights-scraper": "0.0.10",
    "processmd": "^4.6.0",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
